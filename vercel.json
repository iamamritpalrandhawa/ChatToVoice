{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/translate",
            "dest": "/index.js"
        },
        {
            "src": "/public/(.+)",
            "dest": "/index.js"
        },
        {
            "src": "/public",
            "dest": "/index.js"
        },
        {
            "src": "/",
            "dest": "/build/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "build/$1"
        }
    ]
}